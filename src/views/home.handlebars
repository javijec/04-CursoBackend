<div>
    <ul class="product-list">
    {{#each products}}
        <li class="product-item">
            <h2 class="product-title">{{this.title}}</h2>
            <p class="product-description">{{this.description}}</p>
            <div class="product-details">
                <span class="product-detail">ID: {{this.id}}</span>
                <span class="product-detail">Código: {{this.code}}</span>
                <span class="product-detail product-price">Precio: ${{this.price}}</span>
                <span class="product-detail">Estado: {{this.status}}</span>
                <span class="product-detail">Stock: {{this.stock}}</span>
                <span class="product-detail">Categoría: {{this.category}}</span>
            </div>
            {{#if this.thumbnails}}
                <img src="{{this.thumbnails.[0]}}" alt="{{this.title}}" class="img-fluid mt-3 rounded">
            {{/if}}
        </li>
    {{/each}}
    </ul>
</div>